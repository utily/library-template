{
	"name": "@utily/library-template",
	"version": "0.0.15",
	"description": "Template library for Typescript.",
	"author": "Utily Contributors",
	"license": "MIT",
	"repository": "https://github.com/utily/library-template",
	"bugs": {
		"url": "https://github.com/utily/library-template/issues"
	},
	"homepage": "https://github.com/utily",
	"private": false,
	"main": "dist/index.js",
	"typings": "dist/index.d.ts",
	"type": "module",
	"git": {
		"tagName": "v${version}"
	},
	"github": {
		"release": true
	},
	"jest": {
		"transform": {
			"^.+\\.(j|t)sx?$": [
				"ts-jest",
				{
					"tsconfig": "./tsconfig.test.json"
				}
			]
		},
		"transformIgnorePatterns": [
			"<rootDir>/node_modules/(?!(cryptly|authly|isoly|gracely|cloudly-http|cloudly-router|cloudly-rest)/.*)"
		],
		"testEnvironment": "node",
		"testRegex": "((\\.|/)(test|spec))(\\.|\\/.+)(jsx?|tsx?)$",
		"testPathIgnorePatterns": [
			"node_modules/",
			"dist/"
		],
		"collectCoverageFrom": [
			"**/*.{ts,tsx,js,jsx}",
			"!**/node_modules/**",
			"!**/dist/**"
		]
	},
	"scripts": {
		"lint": "eslint --max-warnings=0 '**/*.{ts,tsx}'",
		"fix": "eslint '**/*.{ts,tsx}' --fix",
		"build": "tsc -p .",
		"test": "jest",
		"prepare": "npm run build",
		"clean": "rimraf dist node_modules coverage"
	},
	"devDependencies": {
		"@types/jest": "^29.4.0",
		"@typescript-eslint/eslint-plugin": "5.54.0",
		"@typescript-eslint/parser": "5.54.0",
		"eslint": "^8.35.0",
		"eslint-plugin-prettierx": "github:utily/eslint-plugin-prettierx#utily-20221229",
		"eslint-plugin-simple-import-sort": "^10.0.0",
		"jest": "^29.5.0",
		"prettierx": "github:utily/prettierx#utily-20221229",
		"rimraf": "^4.3.0",
		"ts-jest": "^29.0.5",
		"typescript": "^4.9.5"
	}
}
